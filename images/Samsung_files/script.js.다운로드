$(function () {
    // visual 슬라이드
    var swiper = new Swiper(".m_visual_swiper", {
        spaceBetween: 30,
        centeredSlides: true,
        autoplay: {
            // delay: 2500,
            delay: 999999999,
            disableOnInteraction: false,
        },
        pagination: {
            el: ".swiper-pagination",
            clickable: true,
        },
        navigation: {
            nextEl: ".swiper-button-next",
            prevEl: ".swiper-button-prev",
        },
    });

    // youmake 슬라이드
    var swiper = new Swiper(".youMakeSwiper", {
        spaceBetween: 30,
        pagination: {
            el: ".swiper-pagination",
            clickable: true,
        },
    });
})